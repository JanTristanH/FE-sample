{
  "_version": "1.40.0",
  "sap.app": {
    "id": "demp.opvhorizon",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap-ux/fiori-elements-writer:ovp",
      "version": "0.3.6",
      "toolsId": "3896c591-0786-44be-ba55-531d65ce0259"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/kpi/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.f": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.suite.ui.generic.template": {},
        "sap.ovp": {},
        "sap.ui.rta": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "demp.opvhorizon.i18n.i18n"
        }
      },
      "mainModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "metadataUrlParams": {
            "sap-value-list": "none"
          }
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [],
      "targets": {}
    }
  },
  "sap.ovp": {
    "globalFilterModel": "mainModel",
    "globalFilterEntityType": "NAST",
    "containerLayout": "resizable",
    "enableLiveFilter": true,
    "considerAnalyticalParameters": false,
    "cards": {

      "list01": {
        "model": "mainModel",
        "template": "sap.ovp.cards.list",
        "settings": {
          "title": "Nast List",
          "entitySet": "NAST",
          "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant",
          "annotationPath": "com.sap.vocabularies.UI.v1.LineItem"
        }
      },

      "chart01": {
        "model": "mainModel",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "title": "Nast Chart",
          "entitySet": "NAST",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#DLVZN"
        }
      },
      "chart02": {
        "model": "mainModel",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "title": "Quantity Chart Date",
          "entitySet": "NAST",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#QUANTITYBYDAY"
        }
      },
      "card_linklist_Carousel": {
        "model": "mainModel",
        "template": "sap.ovp.cards.linklist",
        "settings": {
          "title": "Recent Cusomers",
          "entitySet": "Customers",
          "listFlavor": "carousel",
          "sortBy": "KUNWE",
          "sortOrder": "ascending",
          "headerAnnotationPath": "com.sap.vocabularies.UI.v1.HeaderInfo#header1"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "analytical"
  }
}